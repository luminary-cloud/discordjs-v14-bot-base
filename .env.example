# Discord Bot Configuration
# Copy this file to `.env` and fill in your values.

# Required
token=YOUR_BOT_TOKEN
clientId=YOUR_APPLICATION_CLIENT_ID

# Optional: command registration scope
# Set to 'global' to register commands for all guilds
# Set to 'guild' to register commands only in a specific guild
COMMANDS_SCOPE=global
GUILD_ID=YOUR_GUILD_ID_IF_USING_GUILD_SCOPE

# MongoDB connection string
MONGO_URI=mongodb://localhost:27017/discord-bot

# Environment: development or production
NODE_ENV=development
